{
    "name": "decodelabs/imprint",
    "description": "PDF conversion API interface",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/atlas": "^0.14.1",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/hydro": "^0.2.0",
        "decodelabs/kingdom": "^0.1.1"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Imprint\\": "src/Imprint/"
        },
        "classmap": [
            "src/Imprint.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
